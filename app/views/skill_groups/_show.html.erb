<div class="modal-dialog">
	<div class="modal-content">
		<div class="modal-header">
			<h3><%= t('skill_group.skill_list_title')%></h3>
		</div>
		<div class="modal-body">
			<ul class="errors"></ul>
			<p><%= @skill_group.name %></p>
			<ul>
				<% @skills.each do |skill| %>
					<li><%= skill.name %></li>
				<% end %>
			</ul>
		</div>
		<div class="modal-footer">
			<%= link_to t('skill_group.close_button'), "#", class: "btn btn-default", data: {dismiss: "modal"} %>
		</div>
	</div>
</div>	